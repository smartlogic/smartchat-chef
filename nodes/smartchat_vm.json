{
  "authorization" : {
    "sudo" : {
      "groups" : [
        "admin",
        "vagrant",
        "sudo"
      ],
      "passwordless" : true
    }
  },
  "nginx" : {
    "version" : "1.4.4-1~precise",
    "worker_processes" : 1
  },
  "postgresql" : {
    "password" : {
      "postgres" : "password"
    }
  },
  "rbenv" : {
    "group_users" : [
      "deploy",
      "vagrant"
    ]
  },
  "monit": {
    "poll_period" : 30,
    "poll_start_delay" : 60
  },
  "ruby_build" : {
    "git_version": "f758900a28e2ed695ad051dbf982e286014b076f"
  },
  "redisio" : {
    "servers" : [
      {
        "port" : "6379",
        "loglevel" : "warning"
      }
    ]
  },
  "run_list": [
    "users_solo::admins",
    "sudo",
    "postgresql::server",
    "postgresql::ruby",
    "redisio::install",
    "redisio::enable",
    "rbenv",
    "rbenv::ruby_build",
    "smartchat",
    "smartchat::database",
    "nginx::repo",
    "nginx",
    "monit"
  ]
}
